{
  "expo": {
    "name": "Product Shop",
    "slug": "product-shop",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.uii.productshop",
      "versionCode": 1
    },
    "description": "This is Albarra's First React Native App",
    "githubUrl": "https://github.com/albarranaufala/product-shop-react-native"
  }
}
